{
  "name": "blind-date",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "start": "turbo run start",
    "dev": "sudo kong start -c kong/dev.conf && NODE_ENV=development turbo run dev",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "db:generate": "prisma generate",
    "db:push": "prisma db push --skip-generate",
    "db:studio": "prisma studio"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "2.8.4",
    "turbo": "1.8.3",
    "dotenv-cli": "7.1.0"
  },
  "packageManager": "yarn@1.22.17",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}